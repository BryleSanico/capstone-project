{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:tunnel": "expo start --tunnel --clear",
    "start:lan": "./scripts/start-dev.sh --lan",
    "start:localhost": "./scripts/start-dev.sh --localhost",
    "start:web": "expo start --web --tunnel",
    "start:web-dev": "DEBUG=expo* expo start --web --tunnel",
    "start:clean": "./scripts/start-dev.sh --clear",
    "dev": "npm run start",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "format:docs": "prettier --write \"docs/**/*.md\"",
    "format:check": "prettier --check .",
    "docs:serve": "mkdocs serve",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy",
    "reset": "expo r -c && npm run start:clean",
    "tunnel": "npm run start:tunnel",
    "setup": "./scripts/setup.sh",
    "diagnose": "./scripts/diagnose.sh",
    "dev:setup": "npm run setup && npm run start",
    "dev:diagnose": "npm run diagnose"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/react-query": "^5.83.0",
    "@ungap/structured-clone": "^1.3.0",
    "expo-blur": "~14.1.4",
    "expo-dev-client": "^6.0.12",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.6",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-location": "~18.1.4",
    "expo-splash-screen": "~0.30.7",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.6",
    "expo-web-browser": "^14.2.0",
    "nativewind": "^4.1.23",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "^0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "^3.6.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@react-native-community/cli": "^12.3.7",
    "@react-native-community/cli-platform-android": "^12.3.7",
    "@react-native-community/cli-platform-ios": "^12.3.7",
    "@react-native/metro-config": "^0.81.1",
    "@types/react": "^19.1.13",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.31.0",
    "eslint-config-expo": "^9.2.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
